{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "aerni/laravel-spotify": "^1.6",
        "alaouy/youtube": "^2.2",
        "algolia/scout-extended": "^1.0",
        "barryvdh/laravel-translation-manager": "^0.5.10",
        "butschster/meta-tags": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "gliterd/laravel-backblaze-b2": "^1.3",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "hisorange/browser-detect": "^4.3",
        "inertiajs/inertia-laravel": "^0.4.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^7.0",
        "laravel/passport": "^9.4",
        "laravel/scout": "8.1",
        "laravel/socialite": "^5.0",
        "laravel/ui": "^2.5",
        "laravelium/sitemap": "7.*",
        "lcobucci/jwt": "^4",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/oauth2-server": "^8.2.3",
        "listennotes/podcast-api": "^1.0",
        "norkunas/youtube-dl-php": "dev-master",
        "owen-oj/laravel-getid3": "^1.3",
        "php-ffmpeg/php-ffmpeg": "^0.19.0",
        "probablyrational/wasabi-storage": "^1.3",
        "pusher/pusher-php-server": "^4.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "stevebauman/location": "^5.2",
        "stripe/stripe-php": "^7.77",
        "symfony/dom-crawler": "^5.3",
        "teamtnt/laravel-scout-tntsearch-driver": "^11.6",
        "yab/laravel-scout-mysql-driver": "^5.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/tinker": "^2.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds"
        ],
        "files": [
            "app/installer/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
